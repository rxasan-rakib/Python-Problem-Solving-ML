a = float(input("Enter 1st number"))
operatir = input("Enter the operator (+,-,*,/)")
b = float(input("Enter 2st number"))

if operatir == '+':
  print("Result : ", a+b)
elif operatir == '-':
  print("Result : ", a-b)
elif operatir == '*':
  print("Result : ", a*b)
elif operatir == '/':
  print("Result : ", a/b)
else:
  print("Invalid Operatior")
